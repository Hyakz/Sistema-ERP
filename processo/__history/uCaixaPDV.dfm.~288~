  object QryCaixa: TZQuery
    Connection = dtmPrincipal.ConexaoDB
    SQL.Strings = (
      'SELECT'
      '    p.produtoID,'
      '    iv.quantidade,'
      '    p.valor as valorUnitario,'
      '    iv.totalProduto,'
      '    p.nome AS produtoNome,'
      '    p.foto,'
      '    v.clienteID,'
      '    c.nome AS clienteNome'
      'FROM'
      '    itensVenda iv'
      'JOIN'
      '    produtos p ON iv.produtoID = p.produtoID'
      'JOIN'
